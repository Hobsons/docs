<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic
  id="topic_itv_mg5_m4">
  <title>DELETE /gemfire-api/v1/{region}/{key}</title>
  <shortdesc>Delete entry for specified key in the region.</shortdesc>
  <body>
    <section>
      <title>Resource URL</title>
      <codeblock>http://&lt;<i>hostname_or_http-service-bind-address</i>>:&lt;<i>http-service-port</i>>/gemfire-api/v1/{region}/{key}</codeblock>
    </section>
    <section>
      <title>Parameters</title>
      <p>None.</p>
    </section>
    <section>
      <title>Example Request</title>
      <codeblock>Request Payload: null
DELETE /gemfire-api/v1/orders/11
Accept: application/json</codeblock>
    </section>
    <section>
      <title>Example Success Response</title>
      <codeblock>Response Payload: null
<b>200 OK</b></codeblock>
    </section>
    <section>
      <title>Error Codes</title>
      <simpletable
        frame="all"
        relcolwidth="1.0* 2.39*"
        id="simpletable_vfq_33v_m4">
        <sthead>
          <stentry>Status Code</stentry>
          <stentry>Description</stentry>
        </sthead>
        <strow>
          <stentry>404 NOT FOUND</stentry>
          <stentry>Returned if the region or specified key is not found.</stentry>
        </strow>
        <strow>
          <stentry>500 INTERNAL SERVER ERROR</stentry>
          <stentry>Error encountered at <keyword keyref="product_name"/> server. Check the HTTP response body for a stack
            trace of the exception. </stentry>
        </strow>
      </simpletable>
    </section>
  </body>
</topic>
